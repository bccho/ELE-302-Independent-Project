load('bounce_02_09z.mat', 't', 'z')
z = z - 20;

%% Raw bounce
figure
ax = gca;
hp = plot(t, z, '.-');
ylabel('Height above ground, mm')
xlabel('Time, s')
hp.MarkerSize = 10;
hp.LineWidth = 0.7;
ax.FontSize = 12;
xlim([0, max(t)])

%% Demonstration of transform
